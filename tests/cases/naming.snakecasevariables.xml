    <error file="naming/general/Wrong.php" line="89">CodeSnifferUtils.Naming.SnakeCaseVariable.InvalidStart</error>
    <error file="naming/general/Wrong.php" line="91">CodeSnifferUtils.Naming.SnakeCaseVariable.InvalidStart</error>
    <error file="naming/variables/Distinctions.php" line="6">CodeSnifferUtils.Naming.SnakeCaseVariable.InvalidCharacters</error>
