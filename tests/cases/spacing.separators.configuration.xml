<?xml version="1.0"?>
<ruleset name="spacing.namespace">
  <autoload>../../src/autoloader.php</autoload>
  <file>../files/spacing/separator/Configuration.php</file>
  <rule ref="RN.Spacing.Separator">
    <properties>
      <property name="includeFunctionCallCommas" value="yes"/>
    </properties>
  </rule>
  <expectations fixable="true">
    <file_count>1</file_count>
    <error file="spacing/separator/Configuration.php" line="2">RN.Spacing.Separator.SpaceBeforeComma</error>
  </expectations>
</ruleset>
